{
    "function": {
        "prefix": "fn",
        "body": [
            "fn ${1:funcName}(${2:arguments}) ${3:returnType} {",
            "    ${4:print('Hello, World!');}",
            "}"
        ],
        "description": "Create a function"
    },
    "if": {
        "prefix": "if",
        "body": [
            "if (${1:condition}) {",
            "    ${2:ifTrue}",
            "}"
        ],
        "description": "Create an if statement"
    },
    "else": {
        "prefix": "else",
        "body": [
            "else {",
            "    ${1:ifFalse}",
            "}"
        ],
        "description": "Create an else statement"
    },
    "if-else": {
        "prefix": "ife",
        "body": [
            "if (${1:condition}) {",
            "    ${2:ifTrue}",
            "} else {",
            "    ${3:ifFalse}",
            "}"
        ],
        "description": "Create an elif statement"
    },
    "for": {
        "prefix": "for",
        "body": [
            "for (${1:condition}) {",
            "    ${2:body}",
            "}"
        ],
        "description": "Create a for loop"
    },
    "let": {
        "prefix": "let",
        "body": [
            "${1:variable} := ${2:value};"
        ],
        "description": "Create a for loop"
    }
}